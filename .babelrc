{
  "plugins": ["babel-plugin-root-import"],
  "env": {
    "development": {
      "presets": ["next/babel"]
    },
    "production": {
      "presets": ["next/babel"]
    },
    "test": {
      "presets": [["env", { "modules": "commonjs" }], "next/babel"],
      "plugins": [
        "babel-plugin-root-import",
        [
          "inline-import",
          {
            "extensions": [".svg"]
          }
        ]
      ]
    }
  }
}
