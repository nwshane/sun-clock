{
  "name": "sun_clock",
  "version": "1.0.0",
  "description": "A command line app for getting the sunset and sunrise times at your current location.",
  "main": "index.js",
  "scripts": {
    "next-dev": "next",
    "dev": "npm-run-all -p next-dev storybook",
    "jest": "jest",
    "jest-watch": "jest --watch",
    "cypress:run": "TZ=America/Santarem cypress run",
    "cypress:open": "TZ=America/Santarem cypress open",
    "test": "npm-run-all -p jest cypress:run",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-prod": "next build && next export -o dist && touch dist/.nojekyll && cp CNAME dist/CNAME",
    "start-prod": "npm run build-prod && http-server ./dist",
    "cypress:prod:run": "TZ=America/Santarem cypress run --config baseUrl=http://localhost:8080",
    "test-prod": "npm-run-all -p jest cypress:prod:run",
    "deploy": "npm run test-prod && npm run build-prod && gh-pages --dist dist --dotfiles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nwshane/sun-clock.git"
  },
  "author": "Nathan Shane",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nwshane/sun-clock/issues"
  },
  "homepage": "https://github.com/nwshane/sun-clock#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "d3-selection": "^1.2.0",
    "d3-selection-multi": "^1.0.1",
    "d3-shape": "^1.2.0",
    "fast-memoize": "^2.5.1",
    "js-joda": "^1.6.2",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.20.1",
    "next": "^6.1.1",
    "query-string": "^6.0.0",
    "react": "^16.4.1",
    "react-dates": "^17.1.0",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.6",
    "react-select": "^1.2.1",
    "repatch": "^1.3.12",
    "rheostat": "^2.2.0",
    "suncalc": "^1.8.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.6",
    "@storybook/addon-links": "^3.4.6",
    "@storybook/addon-storyshots": "^3.4.6",
    "@storybook/react": "^3.4.6",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-root-import": "^5.1.0",
    "css-loader": "^0.28.9",
    "cypress": "^2.0.1",
    "dotenv": "^5.0.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "gh-pages": "^1.1.0",
    "http-server": "^0.11.1",
    "ignore-loader": "^0.1.2",
    "jest": "^23.1.0",
    "npm-run-all": "^4.1.2",
    "prettier": "1.8.2",
    "raf": "^3.4.0",
    "style-loader": "^0.20.2",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^1.0.1"
  },
  "jest": {
    "setupFiles": [
      "./jestSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/cypress/"
    ]
  }
}
