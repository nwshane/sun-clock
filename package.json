{
  "name": "sun_clock",
  "version": "1.0.0",
  "description": "A command line app for getting the sunset and sunrise times at your current location.",
  "main": "index.js",
  "scripts": {
    "next-dev": "next",
    "dev": "npm-run-all -p next-dev storybook",
    "deploy": "next build && next export -o dist && touch dist/.nojekyll && cp CNAME dist/CNAME && gh-pages --dist dist --dotfiles",
    "start": "next start",
    "test": "npm-run-all -p jest cypress:run",
    "jest": "jest",
    "jest-watch": "jest --watch",
    "cypress:run": "TZ=America/Santarem cypress run",
    "cypress:open": "TZ=America/Santarem cypress open",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nwshane/sun-clock.git"
  },
  "author": "Nathan Shane",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nwshane/sun-clock/issues"
  },
  "homepage": "https://github.com/nwshane/sun-clock#readme",
  "dependencies": {
    "d3-selection": "^1.2.0",
    "d3-selection-multi": "^1.0.1",
    "d3-shape": "^1.2.0",
    "js-joda": "^1.6.2",
    "moment": "^2.20.1",
    "next": "^5.0.0",
    "react": "^16.1.1",
    "react-datepicker": "^1.2.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "repatch": "^1.3.12",
    "suncalc": "^1.8.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.19",
    "@storybook/addon-links": "^3.2.19",
    "@storybook/addon-storyshots": "^3.2.19",
    "@storybook/react": "^3.2.19",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-transform-decorators": "^6.24.1",
    "cypress": "^2.0.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "gh-pages": "^1.1.0",
    "jest": "^21.2.1",
    "npm-run-all": "^4.1.2",
    "prettier": "1.8.2",
    "raf": "^3.4.0"
  },
  "jest": {
    "setupFiles": [
      "./jestSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/cypress/"
    ]
  }
}
